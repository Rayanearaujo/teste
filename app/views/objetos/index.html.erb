<p id="notice"><%= notice %></p>

<h1>Listing Objetos</h1>

<table>
  <thead>
    <tr>
      <th>Crime</th>
      <th>Vitima</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objetos.each do |objeto| %>
      <tr>
        <td><%= objeto.crime %></td>
        <td><%= objeto.vitima %></td>
        <td><%= objeto.descricao %></td>
        <td><%= link_to 'Show', objeto %></td>
        <td><%= link_to 'Edit', edit_objeto_path(objeto) %></td>
        <td><%= link_to 'Excluir', objeto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Objeto', new_objeto_path %>
